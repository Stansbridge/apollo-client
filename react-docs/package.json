{
  "name": "hexo-site",
  "version": "0.0.0",
  "private": true,
  "hexo": {
    "version": "3.2.2"
  },
  "dependencies": {
    "handlebars": "^4.0.5",
    "hexo": "^3.2.0",
    "hexo-generator-archive": "^0.1.2",
    "hexo-generator-category": "^0.1.2",
    "hexo-generator-index": "^0.1.2",
    "hexo-generator-tag": "^0.1.1",
    "hexo-renderer-ejs": "^0.1.0",
    "hexo-renderer-less": "^0.2.0",
    "hexo-renderer-marked": "^0.2.4",
    "hexo-server": "^0.1.2",
    "hexo-typescript-api-box": "^0.5.0",
    "lodash": "^4.13.1",
    "showdown": "^1.4.2",
    "typedoc": "github:tmeasday/typedoc#add-source-to-json-built"
  },
  "devDependencies": {
    "hexo-s3-deploy": "^1.2.1"
  },
  "scripts": {
    "build": "cd code; typedoc --json ../docs.json",
    "prestart": "npm run build",
    "start": "hexo serve",
    "predeploy": "npm run build",
    "deploy": "hexo-s3-deploy",
    "develop-theme": "nodemon -x 'rm db.json; hexo serve' -w assets/ -w code/ -w source/ -w themes/ -w scripts/"
  }
}
